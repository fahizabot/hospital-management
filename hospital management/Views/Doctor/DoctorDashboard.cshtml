@model List<DataAccessLayer.patientdata>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DoctorDashboard</title>
</head>
<body>
    <div>
        <table border="1">
            <tr>
                <td><p>Status</p></td>
                <td><p>Timings</p></td>
                <td><p>Slot</p></td>
                <td><p>Description</p></td>
                <td><p>Pay</p></td>
              
            </tr>

            @{
                foreach (var i in Model)
                {
                    <tr>
                        <td>@i.Status</td>
                        <td>@i.Timings</td>
                        <td>@i.Slot</td>
                        <td>@i.Description</td>
                        <td>@i.Pay</td>
                        <td><a href="~/Doctor/ConfirmPatient?logid=@i.LoginId">confirm</a></td>
                        <td><a href="~/Doctor/CancelPatient">cancel</a></td>
                        <td><a href="~/Doctor/DoctorPrescription">add prescription</a></td>
                    </tr>
                }
            }

        </table>
    </div>
</body>
</html>
