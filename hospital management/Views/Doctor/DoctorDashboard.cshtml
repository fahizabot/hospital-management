@model List<DataAccessLayer.patientdata>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DoctorDashboard</title>
</head>
<body>
    <div>
        <table>

            @{
                foreach (var i in Model)
                {
                        <tr>
                            <td>@i.UserName</td>
                            <td>@i.Age</td>
                            <td>@i.Timing</td>
                            <td>@i.Slot</td>
                            <td>@i.Desciption</td>
                            <td>@i.Status</td>
                            <td>@i.Pay</td>
                            <td><a href="~/Doctor/ConfirmPatient">confirm</a></td>
                            <td><a href="~/Doctor/CancelPatient">cancel</a></td>
                            <td><a href="~/Doctor/DoctorPrescription">add prescription</a></td>
                        </tr>
                }
            }

        </table>
    </div>
</body>
</html>
